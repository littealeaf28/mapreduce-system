# Generate the Go code
go get -u google.golang.org/protobuf/cmd/protoc-gen-go
export PATH="$PATH:$(go env GOPATH)/bin"
protoc --go_out=. --go_opt=paths=source_relative --go-grpc_out=. --go-grpc_opt=paths=source_relative mr/rpc.proto

go get -u google.golang.org/grpc/cmd/protoc-gen-go-grpc

go build -buildmode=plugin mr_apps/wc.go

protoc --go_out=. --go_opt=paths=source_relative mr/kva.proto
